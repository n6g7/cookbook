  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "webpack-dashboard -- webpack-dev-server --config webpack.dev.config.js",
    "pretest": "eslint src test --ext js,jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/n6g7/cookbook.git"
  },
  "keywords": [
    "cook",
    "recipe",
    "cooking",
    "cookbook",
    "ingredients"
  ],
  "author": "Nathan Gaberel <nathan@gnab.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n6g7/cookbook/issues"
  },
  "homepage": "https://github.com/n6g7/cookbook#readme",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "img-loader": "^1.3.1",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
