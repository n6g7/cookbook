    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^1.16.2",
    "yaml-loader": "^0.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
